      "\\begin{equation}\n",
      "    \\left\\{\n",
      "        \\begin{array}{ll}\n",
      "            \\dot{x}(t)=f(x(t)), \\hspace{1cm} t \\in [0,T'] \\\\\n",
      "            x(0)=x_0.\n",
      "        \\end{array}\n",
      "    \\right.\n",
      "\\end{equation}\n",
